{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/form", "dest": "/form" },
      { "src": "/form-send", "dest": "/form-send" },
      { "src": "/api/(.*)", "dest": "/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  